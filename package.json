{
  "name": "mort-mortgage-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "dev:seed": "ts-node --transpile-only prisma/seed.ts",
    "dev:seed-caseworkers": "ts-node --transpile-only prisma/seed-caseworker-data.ts",
    "dev:reset": "node ./scripts/reset-db.js",
    "resume": "npm run dev:reset && npm run dev:seed",
    "test": "jest",
    "test:schemas": "jest src/__tests__/schema*.test.ts --runInBand",
    "test:ocr": "jest src/__tests__/ocr-integration.test.ts --runInBand --testTimeout=120000",
    "generate:docs": "tsx scripts/generate-test-documents.ts",
    "e2e": "cypress open",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "generate:types": "node ./scripts/generate-types.js",
    "gen:types": "npm run generate:types"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "*",
    "@testing-library/react": "*",
    "@types/jest": "^29.5.14",
    "@types/leaflet": "^1.9.21",
    "@types/node": "*",
    "@types/pdfmake": "^0.3.0",
    "@types/react": "*",
    "autoprefixer": "^10.4.24",
    "canvas": "^3.2.1",
    "cypress": "*",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "*",
    "prisma": "5.22.0",
    "tailwindcss": "^3.4.19",
    "ts-jest": "^29.2.5",
    "ts-node": "*",
    "tsx": "^4.21.0",
    "typescript": "*"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.73.0",
    "@prisma/client": "5.22.0",
    "ajv": "*",
    "ajv-formats": "*",
    "bcryptjs": "^3.0.3",
    "formidable": "^3.5.4",
    "json-schema-to-typescript": "*",
    "leaflet": "^1.9.4",
    "next": "*",
    "next-auth": "^4.24.13",
    "pdf-lib": "^1.17.1",
    "pdfmake": "^0.3.3",
    "plaid": "^26.0.0",
    "react": "*",
    "react-dom": "*",
    "react-leaflet": "^4.2.1",
    "react-plaid-link": "^3.6.1",
    "recharts": "^3.7.0",
    "signature_pad": "^5.1.3",
    "swr": "^2.4.0",
    "tesseract.js": "^7.0.0"
  }
}
